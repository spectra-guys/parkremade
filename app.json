{
    "name": "Park Stream",
    "description": "Streams an image to a channel on Twitch to park the stream.",
    "keywords": [
        "twitch",
        "stream",
        "extension",
        "review"
    ],
    "repository": "https://github.com/freaktechnik/park-stream",
    "env": {
        "TWITCH_STREAMKEY": {
            "description": "Twitch Stream key (found at https://www.twitch.tv/dashboard/settings)",
            "required": true
        }
    },
    "stack": "container",
    "formation": {
        "worker": {
            "quantity": 1,
            "size": "free"
        }
    }
}